#shorterner-index
  %table
    %thead
      %tr
        %th Shortened
        %th Full Url
        %th Set Count
        %th Clicked Count
    %tbody
      - @shortens.each do |short|
        %tr
          %td= short['shortened']
          %td= short['url'][0..50] + ((short['url'].length > 50) ? '...' : '')
          %td= short['set-count']
          %td= short['click-count']

